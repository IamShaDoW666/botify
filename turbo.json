{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "db:migrate:dev": {
      "cache": false,
      "persistent": true
    },
    "db:push": {
      "cache": false
    },
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "cache": false
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
